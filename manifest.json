{
  "name": "Round",
  "description": "Track and provide feedback on news consumption",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*", "http://www.nytimes.com/*", "https://www.buzzfeed.com/*"],
    "js": ["/lib/jquery-min.js", "getnytmeta.js", "getbfmeta.js"]
  }],
  "browser_action": {
    "default_title": "Round",
    "default_icon": {
            "19": "images/icon_19.png",
            "38": "images/icon_38.png"
          },
    "default_popup": "pages/popup.html",
    "js": "pages/js/popup.js"
  },
  "background": {
    "scripts": ["/lib/jquery-min.js", "background.js"]
  },
  "permissions": [ "history", "tabs", "storage", "http://*/*", "https://*/*" ]
}