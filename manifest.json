{
  "name": "Round",
  "description": "Analyse and provide feedback on news consumption",
  "version": "1.0",
  "manifest_version": 2,
  // "content_scripts": [
  //     {
  //       "matches": ["http://*/*", "https://*/*"],
  //       "js": ["jquery-min.js"]
  //     }
  //   ],
    "browser_action": {
    "default_title": "Round",
    "default_icon": {
            "19": "images/icon_19.png",
            "38": "images/icon_38.png"
          },
    "default_popup": "pages/popup.html"
  },
  "background": {
    "scripts": ["/background/analytics.js"]
  },
  "permissions": [ "history", "tabs", "storage", "http://*/*", "https://*/*" ]
}
